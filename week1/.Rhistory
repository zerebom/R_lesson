#一様分布の乱数
n=c(3,7,10)
m=10000
Y=numeric(m)
for(i in 1:3){
for(j in 1:m){
Y[j]=max(runif(n[i], min=0, max=1))
}
cat("n =", n[i],"\n")
cat("var =", var(Y),"\n")
cat("median =", median(Y),"\n")
}
